{
  "name": "@leanapp/leanes-server",
  "version": "1.0.0",
  "description": "LeanES application as server example",
  "license": "AGPL-3.0",
  "scripts": {
    "watch": "broccoli serve --output-path dev",
    "dev": "nodemon -L --delay 10ms --watch dev/ --require source-map-support/register --trace-warnings --inspect=0.0.0.0:9229 index.js",
    "dev_build": "broccoli build --output-path dev",
    "dev_migrate": "ENV=development node --require source-map-support/register --trace-warnings migrate",
    "dev_rollback": "ENV=development node --require source-map-support/register --trace-warnings rollback",
    "build": "broccoli build --prod --output-path app",
    "migrate": "node migrate",
    "rollback": "node rollback",
    "start": "pm2 start index.js",
    "pm2": "pm2",
    "test": "mocha",
    "test_build": "ENV=build mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeanApp/leanes-server.git"
  },
  "author": "Denis Trayzhon <d.g.trayzhon@gmail.com>",
  "contributors": [],
  "directories": {
    "app": "app",
    "test": "test"
  },
  "keywords": [
    "MVC",
    "PureMVC",
    "LeanES",
    "leanes-app"
  ],
  "configuration": {
    "mongodb": {
      "default": {
        "host": "192.168.2.2",
        "port": "27017",
        "dbName": "dbName",
        "username": "root",
        "password": "password"
      },
      "description": "Configs for db connection",
      "type": "json"
    },
    "corsOrigins": {
      "description": "CORS origins config",
      "type": "string",
      "default": "*"
    },
    "port": {
      "description": "port for start example server",
      "type": "number",
      "default": 3001
    },
    "silent": {
      "description": "silent console logs",
      "type": "boolean",
      "default": false
    },
    "trustProxy": {
      "description": "trustProxy for start example server",
      "type": "boolean",
      "default": false
    },
    "subdomainOffset": {
      "description": "subdomainOffset for start example server",
      "type": "number",
      "default": 2
    },
    "secret": {
      "description": "Secret key for encoding and decoding tokens.",
      "type": "string",
      "default": "secret"
    },
    "cookieKey": {
      "description": "Secret key for sign cookies.",
      "type": "string",
      "default": "secret"
    },
    "sessionCookie": {
      "description": "Session cookie name.",
      "type": "string",
      "default": "sid"
    },
    "resetTokenTTL": {
      "description": "Reset token time-to-live (in seconds)",
      "type": "number",
      "default": 86400
    },
    "apiKey": {
      "description": "Secret key allowing interactions between servers",
      "type": "string",
      "default": "UUyCtx=d_!%95!Kr2rN+gt8J^Zx7#qz%%XA3tx=d_!%afGpeh822Wh34T7ngZ"
    },
    "clientKey": {
      "description": "Secret key allowing interactions between client and server",
      "type": "string",
      "default": "tx=d_!tx=d_hUUgt8J^Zx%afGyC7#qz%%XA3822Wh34!%95!Kr2rN+peT7ngZ"
    },
    "adminEmail": {
      "description": "Admin email.",
      "type": "string",
      "default": "admin@leanes.com"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-decorators": "^7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.9.0",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.11.5",
    "babel-plugin-flow-runtime": "^0.19.0",
    "babel-plugin-globals": "^3.0.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-parameter-decorator": "1.0.16",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "broccoli": "^3.4.2",
    "broccoli-babel-transpiler": "7.8.0",
    "broccoli-cli": "1.0.0",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-source": "3.0.0",
    "broccoli-string-replace": "0.1.2",
    "chai": "4.2.0",
    "flow-runtime": "^0.17.0",
    "glob": "7.1.6",
    "lodash": "^4.17.19",
    "mocha": "^8.1.3",
    "sinon": "^9.0.3",
    "sinon-chai": "3.5.0"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@leansdk/leanes": "https://github.com/LeanSDK/leanes.git#feature/application_module_improvements",
    "@leansdk/leanes-configurable-addon": "https://github.com/LeanSDK/leanes-configurable-addon.git#develop",
    "@leansdk/leanes-fs-utils-addon": "https://github.com/LeanSDK/leanes-fs-utils-addon.git#develop",
    "@leansdk/leanes-mapper-addon": "https://github.com/LeanSDK/leanes-mapper-addon.git#develop",
    "@leansdk/leanes-mongo-addon": "https://github.com/LeanSDK/leanes-mongo-addon.git",
    "@leansdk/leanes-restful-addon": "https://github.com/LeanSDK/leanes-restful-addon.git#develop",
    "@leansdk/leanes-swagger-addon": "https://github.com/LeanSDK/leanes-swagger-addon.git#develop",
    "commander": "^6.2.0",
    "cron": "1.8.2",
    "json-joi-converter": "^17.2.1-a",
    "nodemon": "1.19.4",
    "pm2": "^3.5.1"
  },
  "bugs": {
    "url": "https://github.com/LeanApp/leanes-server/issues"
  },
  "homepage": "https://github.com/LeanApp/leanes-server#readme"
}
